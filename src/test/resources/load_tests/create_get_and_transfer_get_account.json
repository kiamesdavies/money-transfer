{
  "scenarioName": "Transfer Core API Test",
  "steps": [
    {
      "name": "get_initial_account_balance_for_5",
      "url": "/account/5",
      "operation": "GET",
      "request": {
      },
      "assertions": {
        "status": 200,
        "body": {
          "balance": "$NOT.NULL"
        }
      }
    },
    {
      "name": "get_initial_account_balance_for_4",
      "url": "/account/4/",
      "operation": "GET",
      "request": {
      },
      "assertions": {
        "status": 200,
        "body": {
          "balance": "$NOT.NULL"
        }
      }
    },
    {
      "name": "transfer_from_4_to_5",
      "url": "/account/4/transfer/5",
      "operation": "POST",
      "request": {
        "body": {
          "amount": 10
        }
      },
      "assertions": {
        "status": 200,
        "body": {
          "transactionId": "$NOT.NULL"
        }
      }
    },
    {
      "name": "get_final_account_balance_for_5",
      "url": "/account/5",
      "operation": "GET",
      "request": {
      },
      "assertions": {
        "status": 200,
        "body": {
          "balance": "$GT.${$.get_initial_account_balance_for_5.response.body.balance}"
        }
      }
    },
    {
      "name": "get_final_account_balance_for_4",
      "url": "/account/4/",
      "operation": "GET",
      "request": {
      },
      "assertions": {
        "status": 200,
        "body": {
          "balance": "$LT.${$.get_initial_account_balance_for_4.response.body.balance}"
        }
      }
    }
  ]
}
